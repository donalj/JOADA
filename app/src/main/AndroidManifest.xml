<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dependency.greendao.test.tinder.idioma">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/european_union"
        android:label="@string/app_name"
        android:roundIcon="@drawable/european_union"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.joada.idioma.SwipeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.joada.idioma.ProfileActivity">
        </activity>

    </application>

</manifest>